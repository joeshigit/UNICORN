{
  "name": "unicorn-dcs-functions",
  "description": "Unicorn DataCaptureSystem Cloud Functions",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "20"
  },
  "main": "lib/index.js",
  "dependencies": {
    "busboy": "^1.6.0",
    "cors": "^2.8.5",
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^4.5.0",
    "googleapis": "^169.0.0"
  },
  "devDependencies": {
    "@types/busboy": "^1.5.3",
    "@types/cors": "^2.8.17",
    "firebase-functions-test": "^3.1.0",
    "typescript": "^5.3.0"
  },
  "private": true
}
